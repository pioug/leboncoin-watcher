{
  "name": "leboncoin-watcher",
  "version": "0.0.1",
  "manifest_version": 2,
  "permissions": [
  "http://*/",
  "storage",
  "notifications"
  ],
  "icons": {
    "512": "/app/icon512.png"
  },
  "options_page": "app/options.html",
  "background": {
    "scripts": [
      "app/jquery.js",
      "app/lodash.js",
      "app/background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "app/icon32.png",
    "default_popup": "app/popup.html"
  }
}

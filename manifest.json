{
  "name": "leboncoin-watcher",
  "version": "0.0.1",
  "manifest_version": 2,
  "permissions": [
  "http://*/",
  "storage",
  "notifications"
  ],
  "options_page": "app/options.html",
  "background": {
    "scripts": [
      "app/jquery.js",
      "app/lodash.js",
      "app/background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Le chineur",
    "default_popup": "app/popup.html"
  }
}
